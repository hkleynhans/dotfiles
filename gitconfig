[alias]
	logdog = log --decorate --oneline --graph
	logdogsimple = log --decorate --oneline --graph --simplify-by-decoration
	consdiff = difftool -y -x \"diff -y --suppress-common-lines -W 160\"
[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[difftool]
	prompt = false

